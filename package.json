{
  "name": "vue-auto-atomic-css",
  "version": "0.0.3",
  "publisher": "wangtianyou",
  "engines": {
    "vscode": "^1.65.0"
  },
  "icon": "autoCSS.png",
  "displayName": "Vue Auto Atomic CSS",
  "description": "Vue 项目中使用自动化的 CSS 文件",
  "keywords": [
    "vue atomic css",
    "vue auto css",
    "auto atomic css",
    "atomic css",
    "auto-atomic-css"
  ],
  "repository": {
    "type": "git",
    "url": "https://https://github.com/balabalapupu/vscode-auto-atomic-css"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vue-auto-atomic-css",
    "onLanguage:vue"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vue-auto-atomic-css",
        "title": "使用 Vue Auto Atomic CSS 自动原子化样式"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/parse5": "^6.0.3",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "espree": "^9.3.1",
    "fs-extra": "^10.0.1",
    "get-css-data": "^2.0.2",
    "html-parse-stringify": "^3.0.1",
    "less": "^4.1.2",
    "parse5": "^6.0.1",
    "read-css": "^0.3.0"
  }
}
